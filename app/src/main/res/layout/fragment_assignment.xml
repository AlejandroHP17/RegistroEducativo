<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_transparent"
    android:padding="@dimen/margin_outer"
    tools:context=".main.ui.activityMain.subject.assignment.AssignmentFragment">

    <include
        android:id = "@+id/include_header_assignment"
        layout="@layout/include_header"/>


    <!-- Button -->
    <include
        android:id = "@+id/include_button_assignment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="@dimen/margin_extra_outer"
        app:layout_constraintBottom_toBottomOf="parent"
        layout="@layout/include_button"/>


</androidx.constraintlayout.widget.ConstraintLayout>